{
  "name": "react-form-with-constraints-native",
  "version": "0.15.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/react-form-with-constraints.git"
  },
  "description": "Simple form validation for React",
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "validation",
    "react-form-with-constraints",
    "react-native"
  ],
  "main": "lib-es5/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/",
    "lib-es5/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf lib lib-es5 coverage .rpt2_cache LICENSE",
    "tsc": "tsc",
    "build": "yarn build:esnext && yarn build:es5",
    "build:esnext": "tsc --project tsconfig.lib.json",
    "build:es5": "tsc --project tsconfig.lib-es5.json",
    "prepublishOnly": "yarn clean && yarn build",
    "prepack": "cp ../../LICENSE . && jscodeshift --transform=../../removeConsoleTransform.ts lib lib-es5",
    "postpack": "rm LICENSE",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react-form-with-constraints": "^0.15.0",
    "react-native": ">=0.62.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/jscodeshift": "^0.7.0",
    "@types/react": "^16.9.26",
    "@types/react-native": "^0.61.23",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.2.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^25.2.3",
    "jscodeshift": "^0.7.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react": "^16.13.1",
    "react-form-with-constraints": "^0.15.0",
    "react-native": "^0.61.5",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
